local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Â®Envy-Hub,Version 1.2", "Synapse")

-- Main
local Main = Window:NewTab("(Main)")
local MainSection = Main:NewSection("Main")



MainSection:NewButton("(Become-The-Flash)", "Tpwalk", function()
    local player = game:GetService("Players").LocalPlayer
    local UIS = game:GetService("UserInputService")
    
    function ShiftPressed()
     local ShiftHeld = UIS:IsKeyDown(Enum.KeyCode.LeftShift)
     
     if ShiftHeld then
      player.Character.Humanoid.WalkSpeed = 18
     end
    end
    
    function ShiftReleased()
     local ShiftHeld = UIS:IsKeyDown(Enum.KeyCode.LeftShift)
    
     if ShiftHeld == false then
      player.Character.Humanoid.WalkSpeed = 11
     end
    end
    
    UIS.InputBegan:Connect(ShiftPressed)
    UIS.InputEnded:Connect(ShiftReleased)
end)

